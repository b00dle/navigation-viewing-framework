		"outputs" : null,
		"uniforms" :
		{
			"sphere_center" : "vec3",
			"sphere_radius" : "float"
		}
	},
	"gbuffer_vertex_stage" : 
	{
		"body" : "gua_position = gua_world_position;\nmy_normal = gua_world_normal;",
