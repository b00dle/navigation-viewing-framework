<?xml version="1.0" encoding="UTF-8"?>

<setup>

  <global>

    <animatecoupling>True</animatecoupling>

  </global>

  <device>

    <type>Spacemouse</type>
    <inputsensor>device-spacemouse</inputsensor>

    <display>nohra_touch_table</display>

    <avatartype>None</avatartype>

    <transmitteroffset>
      <x>0.0</x>
      <y>0.043</y>
      <z>1.6</z>
      <rx>0.0</rx>
      <ry>0.0</ry>
      <rz>0.0</rz>
    </transmitteroffset>

    <notrackingposition>
      <x>0.0</x>
      <y>1.75</y>
      <z>0.0</z>
    </notrackingposition>

    <platformpos>
      <x>0.0</x>
      <y>5.0</y>
      <z>0.0</z>
    </platformpos>

    <platformrot>
      <yaw>0.0</yaw>
    </platformrot>

    <groundfollowing>
      <activated>False</activated>
      <raystartheight>0.75</raystartheight>
    </groundfollowing>

    <movementtraces>False</movementtraces>

  </device>

  <!-- First User -->
  
  <user>

    <vip>False</vip>

    <startplatform>0</startplatform>

    <warnings>False</warnings>

  </user>
   
</setup>
